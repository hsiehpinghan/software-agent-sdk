{
    "version": "0.2.0",
    "configurations": [
      {
          "name": "Python: Current File",
          "type": "debugpy",
          "request": "launch",
          "program": "${file}",
          "cwd": "${workspaceFolder}",
          "env": {
            "PYTHONPATH": "${workspaceFolder}/openhands-sdk:${workspaceFolder}/openhands-tools:${env:PYTHONPATH}"
          },
          "console": "integratedTerminal",
          "justMyCode": false
      }
    ]
}